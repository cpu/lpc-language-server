{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es2020"],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": false,
		"outDir": "out",
		"rootDir": "src"
	},
	"ts-node": {
		"esm": true,
		"experimentalSpecifierResolution": "node"
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"]
}
