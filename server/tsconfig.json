{
	"compilerOptions": {		
		"target": "ES2022",
		"lib": ["ES2022"],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"sourceMap": true,
		"strict": false,
		"outDir": "out",
		"rootDir": "src",
		"resolveJsonModule": true,
	},	
	
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"]
}
